{
  "$schema": "http://json.schemastore.org/launchsettings.json",
  "profiles": {
    "Interactive": {
      "commandName": "Project",
      "commandLineArgs": "",
      "workingDirectory": "$(SolutionDir)samples\\onedrive",
      "environmentVariables": {
        "DOTNET_ENVIRONMENT": "Development"
      }
    },
    "Revela Help": {
      "commandName": "Project",
      "commandLineArgs": "--help",
      "workingDirectory": "$(SolutionDir)debug",
      "environmentVariables": {
        "DOTNET_ENVIRONMENT": "Development"
      }
    },
    "Plugin List": {
      "commandName": "Project",
      "commandLineArgs": "plugin list",
      "workingDirectory": "$(SolutionDir)debug",
      "environmentVariables": {
        "DOTNET_ENVIRONMENT": "Development"
      }
    },
    "Source Help": {
      "commandName": "Project",
      "commandLineArgs": "source --help",
      "workingDirectory": "$(SolutionDir)debug",
      "environmentVariables": {
        "DOTNET_ENVIRONMENT": "Development"
      }
    },
    "OneDrive Help": {
      "commandName": "Project",
      "commandLineArgs": "source onedrive --help",
      "workingDirectory": "$(SolutionDir)debug",
      "environmentVariables": {
        "DOTNET_ENVIRONMENT": "Development"
      }
    },
    "OneDrive Download (Debug)": {
      "commandName": "Project",
      "commandLineArgs": "source onedrive download",
      "workingDirectory": "$(SolutionDir)debug",
      "environmentVariables": {
        "DOTNET_ENVIRONMENT": "Development"
      }
    },
    "OneDrive Download (Silent)": {
      "commandName": "Project",
      "commandLineArgs": "source onedrive download",
      "workingDirectory": "$(SolutionDir)debug",
      "environmentVariables": {
        "DOTNET_ENVIRONMENT": "Development",
        "LOGGING__LOGLEVEL__DEFAULT": "Error"
      }
    },
    "OneDrive Init": {
      "commandName": "Project",
      "commandLineArgs": "source onedrive init",
      "workingDirectory": "$(SolutionDir)debug",
      "environmentVariables": {
        "DOTNET_ENVIRONMENT": "Development"
      }
    },
    "Init Project": {
      "commandName": "Project",
      "commandLineArgs": "init project",
      "workingDirectory": "$(SolutionDir)debug",
      "environmentVariables": {
        "DOTNET_ENVIRONMENT": "Development"
      }
    }
  }
}
